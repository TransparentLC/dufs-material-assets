    "vue-router": "^4.3.2",
    "vuetify": "^3.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^5.2.11",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vuetify": "^2.0.3"
  },
